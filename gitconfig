[alias]
    a = add
    aa = add -A
    b = branch
    check = diff --cached
    cm = checkout master
    co = checkout
    fix = commit --am
    l = log --pretty=format:"%Cgreen%h%Creset %Cblue%ad%Creset %s%C(yellow)%d%Creset %Cred[%an]%Creset" --graph --date=short
    llog = log --graph --decorate --name-status
    logp = log --graph --decorate -p --word-diff
    rework = rebase --interactive HEAD~10
    wcheck = diff --cached --color-words
    wdiff = diff --color-words
    wshow = show --color-words
[branch]
    autosetuprebase = always
[color]
    diff = auto
    status = auto
    branch = auto
    grep = auto
[core]
    editor = vim
    excludesfile = ~/.gitignore
[credential]
    helper = cache --timeout 0
[push]
    default = upstream
[rerere]
    enabled = 1
[user]
    email = student@appacademy.io
    name = App Academy Student
